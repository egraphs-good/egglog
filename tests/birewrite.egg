(datatype Math (Add Math Math) (Lit i64))

(birewrite (Add (Add x y) z) (Add x (Add y z)))

(define a (Lit 1))
(define b (Lit 2))
(define c (Lit 3))

(define d (Lit 4))
(define e (Lit 5))
(define f (Lit 6))

(define ex1 (Add (Add a b) c))
(define ex2 (Add d (Add e f)))

(run 10)
(check (= ex1 (Add a (Add b c))))
(check (= ex2 (Add (Add d e) f)))
