

(function Add (i64 i64) i64)


(set (Add 1 1) 2)

(replace (Add 1 1) (Add 1 1) 3)

(fail (check (= (Add 1 1) 2)))

(check (= (Add 1 1) 3))



(function Sub (i64 i64) i64)

(set (Add 2 2) 3)

