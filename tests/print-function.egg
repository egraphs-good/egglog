(relation R (i64 i64))
(R 1 2)
(R 2 3)
(R 3 4)

(print-function R)
(print-function R 3)
(print-function R 3 :file "output.R3.log")
(print-function R :file "output.R.log")
(print-function R 3 :file "output.R3.csv.log" :mode csv)
(print-function R :file "output.R.csv.log" :mode csv)

(datatype E (F i64))
(function G (E String) i64 :no-merge)
(set (G (F 1) "1") 1)
(set (G (F 2) "2") 2)
(set (G (F 3) "3") 3)

(print-function G)
(print-function G 3)
(print-function G 3 :file "output.G3.log" :mode default)
(print-function G :file "output.G.log")
(print-function G 3 :file "output.G3.csv.log" :mode csv)
(print-function G :file "output.G.csv.log" :mode csv)